<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="@drawable/bg_search_panel"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:paddingBottom="3.0dp"
        android:paddingTop="3dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="3.0dp"
            android:layout_marginRight="3.0dp"
            android:background="@drawable/bg_white">
            <ImageView
                android:id="@+id/search_icon"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:padding="4dp"
                android:scaleType="fitCenter"
                android:src="@drawable/search_icon"/>
            <ImageView
                android:id="@+id/search_del"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="4dp"
                android:scaleType="fitCenter"
                android:src="@drawable/btn_del_search"/>
            <EditText
                android:id="@+id/edit_input_number"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dp"
                android:layout_marginRight="5.0dp"
                android:layout_toLeftOf="@id/search_del"
                android:layout_toRightOf="@id/search_icon"
                android:background="@null"
                android:hint="@string/hint_search_text"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:maxLines="1"
                android:textSize="15sp"/>
        </RelativeLayout>
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/v_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1.0">
        <com.external.xlistview.XListView
            android:id="@+id/list_contacts"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffe8e8e8"
            android:cacheColorHint="#ffffffff"
            android:descendantFocusability="afterDescendants"
            android:divider="@color/line"
            android:dividerHeight="0.5dp"
            android:scrollingCache="true"/>
        <TextView
            android:id="@+id/fast_position"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_margin="34dp"
            android:background="@drawable/index_bg"
            android:gravity="center"
            android:padding="2dp"
            android:textColor="#404040"
            android:textSize="48sp"
            android:visibility="invisible"/>
        <com.lrcall.ui.customer.QuickAlphabeticBar
            android:id="@+id/fast_scroller"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:background="@null"
            android:paddingRight="10dp"/>
        <LinearLayout
            android:id="@+id/layout_letters"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">
            <TextView
                android:id="@+id/v_pound"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:freezesText="true"
                android:gravity="center"
                android:text="#"
                android:textColor="#ff999999"
                android:textSize="10sp"/>
            <TextView
                android:id="@+id/v_a"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
