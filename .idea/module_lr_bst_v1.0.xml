  </path>
  
  <path id="lr_bst_v1.0.module.production.classpath"/>
  
  <path id="lr_bst_v1.0.runtime.production.module.classpath"/>
  
  <path id="lr_bst_v1.0.module.classpath"/>
  
  <path id="lr_bst_v1.0.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.lr_bst_v1.0">
