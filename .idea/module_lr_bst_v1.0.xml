  </patternset>
  
  <patternset id="excluded.from.compilation.lr_bst_v1.0">
    <patternset refid="excluded.from.module.lr_bst_v1.0"/>
  </patternset>
  
  
  <target name="compile.module.lr_bst_v1.0" depends="compile.module.lr_bst_v1.0.production,compile.module.lr_bst_v1.0.tests" description="Compile module lr_bst_v1.0"/>
  
