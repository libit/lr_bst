<?xml version="1.0" encoding="UTF-8"?>
<project name="module_lr_bst_v1.0" default="compile.module.lr_bst_v1.0">
  <dirname property="module.lr_bst_v1.0.basedir" file="${ant.file.module_lr_bst_v1.0}"/>
  
  <property name="module.jdk.home.lr_bst_v1.0" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.lr_bst_v1.0" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.lr_bst_v1.0" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.lr_bst_v1.0" value="-encoding UTF-8 -source 1.6 -target 1.6 ${compiler.args}"/>
  
  <property name="lr_bst_v1.0.output.dir" value="undefined"/>
  <property name="lr_bst_v1.0.testoutput.dir" value="undefined"/>
  
  <path id="lr_bst_v1.0.module.bootclasspath">
